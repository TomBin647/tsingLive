# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
# Uncomment this line if you're using Swift
# use_frameworks!

target 'Live' do

#MP3转码
pod 'mp3lame-for-ios'
#自动布局
pod 'Masonry'
#UITableViewCell自动计算高度,配合自动布局使用
pod 'UITableView+FDTemplateLayoutCell'
#UITableView扩展工具
pod 'CBTableViewDataSource'
#等待之时器
pod 'MBProgressHUD'
#网络请求库
pod 'AFNetworking'
#图片异步加载
pod 'SDWebImage'
#自定义库,集成各种常用工具类,宏 https://github.com/bing6/KSToolkit
pod 'KSToolkit'
#刷新组件 https://github.com/bing6/KSRefresh
pod 'KSRefresh'
#启动引导 https://github.com/bing6/KSGuide
pod 'KSGuide'
#本地数据存储 https://github.com/bing6/FMDBDataTable
pod 'FMDBDataTable'
#分段菜单
pod 'HMSegmentedControl'
pod 'BarrageRenderer'
#pod 'GPUImage', '~> 0.1.7' 由于LFLiveKit里面已经集成了GPUImage
pod 'LFLiveKit'
pod 'SDWebImage'
pod 'MJExtension'

end
